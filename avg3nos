//To find averge of 3 intergers 
#include<math.h>
#include<stdio.h>
int main()
{
   
   int a,b,c;
  float avg;
  printf("Enter first integer: ");
    scanf("%d", &a);
    
    printf("Enter second integer: ");
    scanf("%d", &b);
    
    printf("Enter third integer: ");
    scanf("%d", &c);
    
    avg = (float)(a+b+c)/3;
    printf("\nAverage of Three Numbers : %f",avg);
    return 0;
}
